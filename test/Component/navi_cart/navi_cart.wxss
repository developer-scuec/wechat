/* Component/navi.wxss */
/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */

 .nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  background: white;
  color: #000;
  z-index: 9999999;

  
} 
.nav-icon{
  display: inline-flex;
  justify-content: flex-start;
  position: absolute;
  
}
.left-icon{
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.left-icon{
  margin-left: 15rpx;
}
.right-icon{
  position: relative;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width:45%;
  height:100%;
  font-family: Google Sans;
  font-size: 35rpx;
  color: #3c4043;
}

